{
  LinearLayoutCompat;
  layout_height = "fill";
  gravity = "center";
  layout_width = "fill";
  orientation = "vertical";
  {
    LinearLayoutCompat;
    layout_height = "wrap_content";
    gravity = "center_horizontal";
    layout_width = "400dp";
    orientation = "horizontal";
    {
      AppCompatImageButton;
      id = "exit";
      backgroundColor = "#00000000";
      layout_height = "45dp";
      layout_width = "45dp";
      src = "exit.png";
    };
    {
      View;
      layout_weight = 1;
      layout_width = "10dp";
      layout_height = "45dp";
    };
    {
      AppCompatImageButton;
      id = "undo";
      backgroundColor = "#00000000";
      layout_height = "45dp";
      layout_width = "45dp";
      src = "undo.png";
    };
    {
      AppCompatImageButton;
      id = "more";
      backgroundColor = "#00000000";
      layout_height = "45dp";
      layout_width = "45dp";
      src = "more.png";
    };
  };
  {
    EditText;
    layout_weight = 1;
    gravity = "top|start";
    layout_height = "0dp";
    id = "editText";
    padding = "16dp";
    hint = "可编辑文字";
    layout_width = "fill";
    background = "@android:color/transparent";
  };
};